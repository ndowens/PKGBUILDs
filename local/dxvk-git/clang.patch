 build-win64.txt   | 10 +++++-----
 package-native.sh |  4 ++--
 2 files changed, 7 insertions(+), 7 deletions(-)

diff --git a/build-win64.txt b/build-win64.txt
index 8eb1b29d..ca2fe5cd 100644
--- a/build-win64.txt
+++ b/build-win64.txt
@@ -1,9 +1,9 @@
 [binaries]
-c = 'x86_64-w64-mingw32-gcc'
-cpp = 'x86_64-w64-mingw32-g++'
-ar = 'x86_64-w64-mingw32-ar'
-strip = 'x86_64-w64-mingw32-strip'
-windres = 'x86_64-w64-mingw32-windres'
+c = 'clang'
+cpp = 'clang++'
+ar = 'llvm-ar'
+strip = 'llvm-strip'
+windres = 'llvm-windres'
 
 [properties]
 needs_exe_wrapper = true
diff --git a/package-native.sh b/package-native.sh
index cbd4132e..52c414c9 100755
--- a/package-native.sh
+++ b/package-native.sh
@@ -28,8 +28,8 @@ opt_buildid=false
 opt_64_only=0
 opt_32_only=0
 
-CC=${CC:="gcc"}
-CXX=${CXX:="g++"}
+CC=${CC:="clang"}
+CXX=${CXX:="clang++"}
 
 while [ $# -gt 0 ]; do
   case "$1" in
